<android.support.v4.widget.NestedScrollView xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    xmlns:android="http://schemas.android.com/apk/res/android">


<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/www"
    tools:context=".ui.DetailWisata">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

                    <ImageView
                        android:id="@+id/gambar_wisata"
                        android:layout_width="match_parent"
                        android:layout_height="290dp"
                        android:scaleType="fitXY"
                        android:fitsSystemWindows="true"
                        tools:ignore="ContentDescription">

                        </ImageView>

                <include layout="@layout/konten_detail_wisata" />

    </LinearLayout>


</RelativeLayout>

    </android.support.v4.widget.NestedScrollView>